<%= form_for(@listing) do |f| %>
  <% if @listing.errors.any? %>
    <div class="alert alert-danger">
      <p><%= pluralize(@listing.errors.count, "error") %> prohibited this listing from being saved:</p>
      <ul>
      <% @listing.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :Venue %>
    <%= f.text_field :venue, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :Game_Date %><br>
    <%= f.date_select :gamedate, class: 'form-control' %>
  </div>
    <div class="form-group">
    <%= f.label :Game_Time%><br>
    <%= f.time_select :gametime, class: 'form-control' %>
  </div>

  
  <%= f.submit class: 'btn btn-primary' %>
  <%= link_to 'Cancel', :back, class: 'btn btn-danger' %>
<% end %>
</div>
